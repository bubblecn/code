/*
 * 测试代码
 */
public class TestState {
	public static void main(String arg[])
	{
		Context con = new Context();
		con.getState();
		con.pull();
		con.getState();
		con.pull();
		con.getState();
		con.pull();
		con.getState();
		con.pull();
		con.getState();
		con.push();
		con.getState();
		con.push();
		con.getState();
		con.push();
		con.getState();
		con.push();
		con.getState();
	}
}
